COMPOSE_PROJECT_NAME=pollen-lms-be
APP_ENV=local
API_PORT=8080

# Pollen Admin
POLLEN_ADMIN_INTERNAL_KEY=key
POLLEN_ADMIN_FE_URL=http://localhost:3000/
POLLEN_ADMIN_FE_KEYCLOAK_CLIENT_ID=pollen-admin

# Postgres
POSTGRES_HOST=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=local
POSTGRES_DB=pollen_lms
POSTGRES_PORT=5432
POSTGRES_LOGGING=error

# Keycloak
KEYCLOAK_AUTH_SERVER_URL=http://localhost:8080/
KEYCLOAK_REALM=PollenPass
KEYCLOAK_CLIENT_ID=lms-api
KEYCLOAK_CLIENT_SECRET=<secret>
